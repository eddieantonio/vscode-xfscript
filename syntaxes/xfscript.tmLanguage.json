{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "XFScript",
	"patterns": [
		{
			"include": "#defines"
		}
	],
	"repository": {
		"defines": {
			"patterns": [
				{
					"name": "meta.define.xfscript",
					"match": "\\b(define)\\s*(\\w+)",
					"captures": {
						"1": { "name": "storage.type.xfscript" },
						"2": { "name": "entity.name.function.xfscript" }
					}
				}
			]
		}
	},
	"scopeName": "source.xfscript"
}